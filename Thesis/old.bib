  number = {1}
}

@inproceedings{nurkAssemblingGenomesMinimetagenomes2013a,
  title = {Assembling {{Genomes}} and {{Mini}}-Metagenomes from {{Highly Chimeric Reads}}},
  booktitle = {Research in {{Computational Molecular Biology}}},
  author = {Nurk, Sergey and Bankevich, Anton and Antipov, Dmitry and Gurevich, Alexey and Korobeynikov, Anton and Lapidus, Alla and Prjibelsky, Andrey and Pyshkin, Alexey and Sirotkin, Alexander and Sirotkin, Yakov and Stepanauskas, Ramunas and McLean, Jeffrey and Lasken, Roger and Clingenpeel, Scott R. and Woyke, Tanja and Tesler, Glenn and Alekseyev, Max A. and Pevzner, Pavel A.},
