@article{sakamotoNewEraLongread2019,
  ids = {sakamotoNewEraLongread2019,sakamotoNewEraLongread2019a,sakamotoNewEraLongread2019c},
  title = {A New Era of Long-Read Sequencing for Cancer Genomics},
  author = {Sakamoto, Yoshitaka and Sereewattanawoot, Sarun and Suzuki, Ayako},
  year = {2019},
  month = sep,
  pages = {1--8},
  issn = {1435-232X},
  doi = {10.1038/s10038-019-0658-5},
  abstract = {Cancer is a disease largely caused by genomic aberrations. Utilizing many rapidly emerging sequencing technologies, researchers have studied cancer genomes to understand the molecular statuses of cancer cells and to reveal their vulnerabilities, such as driver mutations or gene expression. Long-read technologies enable us to identify and characterize novel types of cancerous mutations, including complicated structural variants in haplotype resolution. In this review, we introduce three representative platforms for long-read sequencing and research trends of cancer genomics with long-read data. Further, we describe that aberrant transcriptome and epigenome statuses, namely, fusion transcripts, as well as aberrant transcript isoforms and the phase information of DNA methylation, are able to be elucidated by long-read sequencers. Long-read sequencing may shed light on novel types of aberrations in cancer genomics that are being missed by conventional short-read sequencing analyses.},
  copyright = {2019 The Author(s)},
  file = {/Users/urbanslug/Zotero/storage/WMPTGF3R/Sakamoto et al. - 2019 - A new era of long-read sequencing for cancer genom.pdf;/Users/urbanslug/Zotero/storage/ZG9LFPEQ/Sakamoto et al. - 2019 - A new era of long-read sequencing for cancer genom.pdf;/Users/urbanslug/Zotero/storage/33UZUYXE/s10038-019-0658-5.html;/Users/urbanslug/Zotero/storage/X8L4LFWN/s10038-019-0658-5.html},
  journal = {Journal of Human Genetics},
  language = {en}
}

@article{schlenderBovineRespiratorySyncytial2000,
