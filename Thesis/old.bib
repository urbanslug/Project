@article{johnsonGlycoproteinHumanRespiratory1987,
  ids = {johnsonGlycoproteinHumanRespiratory1987,johnsonGlycoproteinHumanRespiratory1987a,johnsonGlycoproteinHumanRespiratory1987c},
  title = {The {{G}} Glycoprotein of Human Respiratory Syncytial Viruses of Subgroups {{A}} and {{B}}: Extensive Sequence Divergence between Antigenically Related Proteins},
  shorttitle = {The {{G}} Glycoprotein of Human Respiratory Syncytial Viruses of Subgroups {{A}} and {{B}}},
  author = {Johnson, P. R. and Spriggs, M. K. and Olmsted, R. A. and Collins, P. L.},
  year = {1987},
  month = aug,
  volume = {84},
  pages = {5625--5629},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.84.16.5625},
  abstract = {Two major antigenic subgroups (designated A and B) have been described for human respiratory syncytial virus (RSV). Previously, on the basis of reactivity patterns with monoclonal antibodies, the greatest intersubgroup variation was shown to occur in the G protein, the putative attachment glycoprotein. To delineate the molecular basis for this variation, we have determined the nucleotide and deduced amino acid sequences of the G mRNAs and proteins representing a subgroup A (Long strain) and a subgroup B (18537 strain) virus. These sequences were compared to the available G mRNA sequence for another subgroup A (A2 strain) virus. The Long G protein shared 94\% amino acid identity with the A2 G protein. In contrast, the 18537 G protein shared only 53\% amino acid identity with the A2 sequence; interestingly, most of the sequence divergence occurred in the proposed extracellular domain of the G protein. This extensive divergence for the G protein was significantly greater than that observed for other RSV proteins. Despite this considerable divarication, the proposed extracellular domains of the G proteins contained a single region of highly conserved sequence and secondary structure that may represent a conserved structural or function domain, perhaps involved in attachment to cellular receptors. Furthermore, this conserved region may comprise part of an epitope that is shared between the two subgroup G proteins and may significantly contribute to the fact that, despite extensive overall amino acid sequence divergence, the RSV G proteins maintain significant antigenic relatedness.},
  file = {/Users/urbanslug/Zotero/storage/R94E5ANA/Johnson et al. - 1987 - The G glycoprotein of human respiratory syncytial .pdf;/Users/urbanslug/Zotero/storage/VKJYK4L7/Johnson et al. - 1987 - The G glycoprotein of human respiratory syncytial .pdf;/Users/urbanslug/Zotero/storage/2SU35IVF/5625.html;/Users/urbanslug/Zotero/storage/JU52VGD9/5625.html},
  journal = {Proceedings of the National Academy of Sciences},
  language = {en},
  number = {16},
  pmid = {2441388}
}

@article{karagiannidisEvolvingEpidemiologyHome2019c,
  ids = {karagiannidisEvolvingEpidemiologyHome2019a,karagiannidisEvolvingEpidemiologyHome2019b},
  title = {{[Evolving Epidemiology of Home Mechanical Ventilation: A Rapidly Growing Challenge for Patient Care].}},
  author = {Karagiannidis, C. and Strassmann, S. and Callegari, J. and Kochanek, M. and Janssens, U. and Windisch, W.},
  year = {2019},
  month = nov,
  volume = {73},
  pages = {670--676},
  issn = {1438-8790 0934-8387},
  doi = {10.1055/a-0976-9119},
  abstract = {BACKGROUND: Home mechanical ventilation is dramatically evolving in Germany. Patients with non-invasive and invasive ventilation are increasingly treated at home. In-hospital treatment of these patients is also necessary either for control visits or the management of acute medical problems. However, the development of in-hospital patient care, morbidity and mortality of these patients is unknown. METHODS: All patients with long-term dependence on mechanical ventilation for more than three months requiring hospitalisation between 2006 and 2016 were analysed (data obtained from the Federal Statistical Office of Germany). RESULTS: There was an exponential increase in the number of in-patients with long-term dependence of mechanical ventilation. While 24 845 patients were treated in-hospital in 2006, 86 117 patients were treated in 2016.  Correspondingly, mortality decreased from 13.2 \% (2006) to 5.7 \% (2016). In addition, in 2016 47 \% of all patients were treated on the intensive care or high dependency care unit. Overall, patients had been severely ill, as there were plenty of medical and neurological co-morbidities. The most common diagnosis was  COPD with 58 \% of all cases, followed by several cardiology diagnosis. A high number of patients had an impairment of renal function (24 \%), in part requiring  dialysis. CONCLUSIONS: The rapid development of home mechanical ventilation substantially impacts on the development of the hospital landscape in Germany. The exponential increase of these care-intensive patients is challenging for the  health care system and requires a discussion about its limits.},
  copyright = {(c) Georg Thieme Verlag KG Stuttgart . New York.},
  journal = {Pneumologie (Stuttgart, Germany)},
  language = {ger},
  number = {11},
  pmid = {31715635}
}

