@article{zhaoSSWLibrarySIMD2013a,
  title = {{{SSW Library}}: {{An SIMD Smith}}-{{Waterman C}}/{{C}}++ {{Library}} for {{Use}} in {{Genomic Applications}}},
  shorttitle = {{{SSW Library}}},
  author = {Zhao, Mengyao and Lee, Wan-Ping and Garrison, Erik P. and Marth, Gabor T.},
  year = {2013},
